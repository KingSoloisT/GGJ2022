{
    escape = true;
    instance_destroy(obj_pickup);
}

if place_meeting(x,y,obj_enemy)
{
    room_restart();
}

if place_meeting(x,y,obj_gate) &amp;&amp; escape = true
{
    room_goto_next();
}




